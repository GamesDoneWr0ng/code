    "def a(n):\n",
    "    return 6 + 12 * n # formel for n-te ledd\n",
    "\n",
    "s = 0 # summen\n",
    "for i in range(1, 201): # for alle n opp til og med 200\n",
    "    s += a(i)           # legger til a_n\n",
    "\n",
    "print(f\"Summen er: {s}\")"
